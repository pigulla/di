  "license": "MIT",
  "scripts": {
    "lint": "eslint --ext=.ts 'packages/**/*.ts'",
    "outdated": "lerna exec --no-bail 'npm outdated'",
    "build": "lerna exec 'rm -rf lib' && lerna run build",
    "setup": "rm -rf node_modules && npm install && lerna exec 'rm -rf node_modules' && lerna exec 'npm install'",
    "test": "npm run lint && npm run test-with-coverage && npm run audit:all",
    "audit:all": "lerna exec 'npm audit'",
    "test-with-coverage": "nyc mocha 'packages/*/test/**/*.spec.ts'"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@pollyjs/adapter-node-http": "^2.6.3",
    "@pollyjs/core": "^2.6.3",
    "@pollyjs/persister-fs": "^2.6.3",
    "@types/chai": "^4.2.4",
    "@types/chai-as-promised": "^7.1.2",
    "@types/mocha": "^5.2.7",
    "@types/pollyjs__adapter-node-http": "^2.0.0",
    "@types/pollyjs__core": "^2.6.1",
    "@types/pollyjs__persister-fs": "^2.0.0",
    "@types/sinon": "^7.5.0",
    "@types/sinon-chai": "^3.2.3",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^6.6.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "json-strictify": "^6.0.0",
    "lerna": "^3.18.3",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.3.0",
    "supertest": "^4.0.2",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.6.4"
  }
}
