{
  "name": "@digitally-imported/meta",
  "version": "0.0.1",
  "private": true,
  "author": "Raphael Pigulla <pigulla@four66.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ext=.ts 'packages/**/*.ts'",
    "outdated": "npm outdated; lerna exec --no-bail 'npm outdated'",
    "build": "lerna exec 'rm -rf lib' && lerna run build",
    "setup": "rm -rf node_modules && npm install && lerna exec 'rm -rf node_modules' && lerna exec 'npm install'",
    "test": "npm run lint && npm run test-with-coverage && npm run audit:all",
    "audit:all": "npm audit && lerna exec 'npm audit'",
    "test-with-coverage": "nyc mocha --file mocha.hooks.ts 'packages/*/test/**/*.spec.ts'",
    "interactive-integration-test": "ts-node --require tsconfig-paths/register test/interactive-integration.spec.ts"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/chai": "^4.2.6",
    "@types/chai-as-promised": "^7.1.2",
    "@types/express": "^4.17.2",
    "@types/mocha": "^5.2.7",
    "@types/sinon": "^7.5.1",
    "@types/sinon-chai": "^3.2.3",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^6.7.2",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "hook-std": "^2.0.0",
    "lerna": "^3.19.0",
    "mocha": "^6.2.2",
    "nock": "^11.7.0",
    "node-mocks-http": "^1.8.0",
    "nyc": "^14.1.1",
    "ora": "^4.0.3",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.3.0",
    "ts-node": "^8.5.4",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.3"
  }
}
