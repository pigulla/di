{
    "name": "@digitally-imported/meta",
    "version": "0.0.4",
    "private": true,
    "workspaces": [
        "packages/*"
    ],
    "author": "Raphael Pigulla <pigulla@four66.com>",
    "homepage": "https://github.com/pigulla/di",
    "repository": {
        "type": "git",
        "url": "https://github.com/pigulla/di.git"
    },
    "bugs": {
        "url": "https://github.com/pigulla/di/issues"
    },
    "license": "MIT",
    "scripts": {
        "setup": "rm -rf node_modules && yarn install && yarn run clean && yarn run build",
        "build": "yarn workspaces foreach --verbose --all --parallel --topological run build",
        "test": "yarn workspaces foreach --verbose --parallel --all run test",
        "clean": "yarn workspaces foreach --verbose --parallel --all run clean",
        "jest": "yarn workspaces foreach --verbose --parallel --all run jest",
        "lint": "yarn workspaces foreach --verbose --parallel --all run lint",
        "format": "yarn workspaces foreach --verbose --parallel --all run format",
        "codecov": "yarn workspaces foreach --verbose --parallel --all run codecov"
    }
}
