{
    "name": "@digitally-imported/cli",
    "version": "0.0.2",
    "description": "di CLI",
    "main": "lib/index.js",
    "keywords": [
        "digitally-imported",
        "cli",
        "oclif"
    ],
    "bin": {
        "di": "./bin/run"
    },
    "scripts": {
        "test": "tsc --noEmit && yarn run lint && yarn run mocha",
        "lint": "eslint --ext=.ts '**/*.ts'",
        "build": "rm -rf lib/ && tsc --build tsconfig.build.json",
        "mocha": "nyc mocha --file test/mocha.hooks.ts 'test/**/*.spec.ts'",
        "clean": "rm -rf node_modules lib .nyc_output coverage",
        "codecov": "bash -c 'bash <(curl https://codecov.io/bash) -Z -f ./coverage/lcov.info -F cli'",
        "postpack": "rm -f oclif.manifest.json",
        "version": "oclif-dev readme && git add README.md",
        "prepack": "yarn run build && oclif-dev manifest"
    },
    "author": "Raphael Pigulla <pigulla@four66.com>",
    "homepage": "https://github.com/pigulla/di/tree/master/packages/cli",
    "bugs": {
        "url": "https://github.com/pigulla/di/issues"
    },
    "license": "MIT",
    "engines": {
        "node": ">=10.0.0"
    },
    "dependencies": {
        "@digitally-imported/client": "^0.0.2",
        "@digitally-imported/dto": "^0.0.2",
        "@oclif/command": "^1.5.19",
        "@oclif/config": "^1.13.3",
        "@oclif/plugin-help": "^2.2.3",
        "cli-ux": "^5.4.1",
        "inquirer": "^7.0.1",
        "json-strictify": "^6.3.2",
        "tslib": "^1.10.0"
    },
    "devDependencies": {
        "@digitally-imported/eslint-config-standard": "^0.0.2",
        "@digitally-imported/mocha-config": "^0.0.2",
        "@digitally-imported/nyc-config": "^0.0.2",
        "@digitally-imported/typescript-config": "^0.0.2",
        "@oclif/dev-cli": "^1.22.2",
        "@oclif/test": "^1.2.5",
        "@types/chai": "^4.2.7",
        "@types/chai-as-promised": "^7.1.2",
        "@types/inquirer": "^6.5.0",
        "@types/mocha": "^5.2.7",
        "@types/node": "^13.0.0",
        "@types/sinon": "^7.5.1",
        "@types/sinon-chai": "^3.2.3",
        "@typescript-eslint/eslint-plugin": "^2.12.0",
        "chai": "^4.2.0",
        "chai-as-promised": "^7.1.1",
        "eslint": "^6.8.0",
        "eslint-config-standard": "^14.1.0",
        "eslint-plugin-import": "^2.19.1",
        "eslint-plugin-node": "^11.0.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-standard": "^4.0.1",
        "mocha": "^6.2.2",
        "nyc": "^15.0.0",
        "sinon": "^8.0.1",
        "sinon-chai": "^3.3.0",
        "ts-node": "^8.5.4",
        "tsconfig-paths": "^3.9.0",
        "type-fest": "^0.8.1",
        "typescript": "^3.7.4"
    },
    "oclif": {
        "commands": "./lib/commands",
        "bin": "di",
        "plugins": [
            "@oclif/plugin-help"
        ]
    },
    "files": [
        "bin",
        "lib",
        "LICENSE",
        "README.md"
    ]
}
