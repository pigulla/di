{
  "name": "@digitally-imported/cli",
  "version": "0.0.1",
  "author": "Raphael Pigulla <pigulla@four66.com>",
  "scripts": {
    "test": "tsc --noEmit && yarn run lint && yarn run mocha",
    "lint": "eslint --ext=.ts '**/*.ts'",
    "build": "rm -rf lib/ && tsc --build tsconfig.build.json",
    "clean": "rm -rf node_modules && rm -rf lib",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "mocha": "nyc mocha --file test/mocha.hooks.ts 'test/**/*.spec.ts'",
    "version": "oclif-dev readme && git add README.md"
  },
  "bin": {
    "di": "./bin/run"
  },
  "bugs": "https://github.com/pigulla/cli/issues",
  "dependencies": {
    "@digitally-imported/client": "^0.0.1",
    "@digitally-imported/dto": "^0.0.1",
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-help": "^2.2.1",
    "chalk": "^3.0.0",
    "csv-stringify": "^5.3.3",
    "inquirer": "^7.0.0",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "json-strictify": "^6.3.0",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@digitally-imported/settings": "^0.0.1",
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@oclif/dev-cli": "^1.22.2",
    "@types/chai": "^4.2.6",
    "@types/chai-as-promised": "^7.1.2",
    "@types/inquirer": "^6.5.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.14",
    "@types/sinon": "^7.5.1",
    "@types/sinon-chai": "^3.2.3",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^6.7.2",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "mocha": "^6.2.2",
    "nock": "^11.7.0",
    "nyc": "^14.1.1",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.3.0",
    "ts-node": "^8.5.4",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.3"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/pigulla/cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "di",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "pigulla/cli",
  "types": "lib/index.d.ts"
}
