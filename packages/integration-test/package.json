{
  "name": "@digitally-imported/integration-test",
  "version": "0.0.1",
  "description": "Non-automatic integration 'tests' for the @digitally-imported components.",
  "main": "lib/interactive-integration.js",
  "keywords": [
    "digitally-imported",
    "integration-test"
  ],
  "scripts": {
    "test": "tsc --noEmit && yarn run lint",
    "lint": "eslint --ext=.ts '**/*.ts'",
    "build": "rm -rf lib/ && tsc --build tsconfig.build.json",
    "start": "node lib/interactive-integration.js",
    "clean": "rm -rf node_modules lib .nyc_output coverage",
    "codecov": "echo 'Not applicable'",
    "dev": "ts-node src/interactive-integration.ts"
  },
  "author": "Raphael Pigulla <pigulla@four66.com>",
  "homepage": "https://github.com/pigulla/di/tree/develop/packages/integration-test",
  "bugs": {
    "url": "https://github.com/pigulla/di/issues"
  },
  "license": "MIT",
  "engines": {
    "node": ">=10.0.0"
  },
  "devDependencies": {
    "typescript": "^3.7.3"
  },
  "dependencies": {
    "module-alias": "^2.2.2",
    "ora": "^4.0.3"
  }
}
