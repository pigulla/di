  "scripts": {
    "test": "tsc --noEmit && yarn run lint && yarn run mocha",
    "lint": "eslint --ext=.ts '**/*.ts'",
    "build": "rm -rf lib/ && tsc --build tsconfig.build.json",
    "mocha": "nyc mocha --file test/mocha.hooks.ts 'test/**/*.spec.ts'",
    "clean": "rm -rf node_modules lib .nyc_output coverage",
    "start:dev": "ts-node -r tsconfig-paths/register src/main.ts",
    "codecov": "bash -c 'bash <(curl https://codecov.io/bash) -Z -f ./coverage/lcov.info -F server'"
  },
