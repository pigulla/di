{
  "name": "@digitally-imported/server",
  "version": "0.0.3",
  "description": "Listen to music from Digitally Imported, control it from the command line and many other features.",
  "main": "lib/main.js",
  "keywords": [
    "digitally-imported",
    "server",
    "backend",
    "nestjs"
  ],
  "bin": {
    "di-server": "./bin/run"
  },
  "scripts": {
    "test": "tsc --noEmit --project tsconfig.json && yarn run lint && yarn run jest",
    "lint": "yarn run eslint && yarn run prettier",
    "eslint": "eslint --ext=.ts src test",
    "prettier": "prettier --check src test",
    "build": "rm -rf lib/ && tsc --build tsconfig.json",
    "clean": "rm -rf lib coverage",
    "jest": "NODE_NO_WARNINGS=1 jest",
    "format": "prettier --write src test && eslint --fix 'src/**/*.ts' 'test/**/*.ts'",
    "codecov": "bash -c 'bash <(curl https://codecov.io/bash) -Z -f ./coverage/lcov.info -F server'",
    "prepack": "yarn run build"
  },
  "author": "Raphael Pigulla <pigulla@four66.com>",
  "homepage": "https://github.com/pigulla/di/tree/master/packages/server",
  "repository": {
    "type": "git",
    "url": "https://github.com/pigulla/di.git"
  },
  "bugs": {
    "url": "https://github.com/pigulla/di/issues"
  },
  "license": "MIT",
  "engines": {
    "node": ">=10.0.0"
  },
