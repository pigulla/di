{
    "compilerOptions": {
        "declaration": true,
        "importHelpers": true,
        "allowJs": false,
        "experimentalDecorators": true,
        "module": "commonjs",
        "strict": true,
        "target": "es2017",
        "esModuleInterop": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noImplicitAny": true,
        "noImplicitThis": true
    }
}
